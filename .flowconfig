[ignore]
.*/styled-components/src/.*
.*/styled-components/lib/native/.*
.*/styled-components/lib/models/InlineStyle.js.flow
.*/styled-components/dangerfile.js
.*/stylelint/.*

[include]

[libs]
interfaces/

[options]
module.system=haste

experimental.strict_type_args=true
munge_underscores=true
unsafe.enable_getters_and_setters=true

esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable

module.name_mapper='^[./a-zA-Z0-9$_-]+\.\(css\|scss\|less\)$' -> 'StyleSheetStub'
module.name_mapper='^[./a-zA-Z0-9$_-]+\.\(bmp\|gif\|jpg\|jpeg\|png\|psd\|svg\|webp\|m4v\|mov\|mp4\|mpeg\|mpg\|webm\|aac\|aiff\|caf\|m4a\|mp3\|wav\|html\|pdf|ttf|otf\)$' -> 'ImageStub'

suppress_type=$FlowIssue
suppress_type=$FlowFixMe
suppress_type=$FixMe

suppress_comment=\\(.\\|\n\\)*\\$FlowIssue
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
